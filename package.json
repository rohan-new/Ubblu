{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "per-env",
    "build:production": "babel src -d dist",
    "lint": "eslint '**/*.js' && npm run prettier:check",
    "lint:fix": "eslint '**/*.js' --fix && npm run prettier:check",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "prettier": "prettier --write *.js \"**/*.js\"",
    "prettier:check": "prettier -c *.js \"**/*.js\"",
    "start": "per-env",
    "start:development": "nodemon -r ./src/env.js -r @babel/register src/app.js",
    "start:production": "node -r ./src/env.js -r @babel/register src/app.js",
    "test": "mocha --exit"
  },
